---
interface Props {
  sampleImageUrls: (string | null)[];
}

const { sampleImageUrls } = Astro.props;
---

<div class="flex flex-col gap-8 lg:gap-10">
  {
    sampleImageUrls.map((sampleImageUrl) => (
      <img
        src={sampleImageUrl}
        alt="sample image"
        class="w-full object-contain"
      />
    ))
  }
</div>
